<?xml version="1.0"?>
<demographics>

  <user code="0x0000">
    <user_id>
      <descryption>id，不重复</descryption>
      <type>INT</type>
      <length>8</length>
      <valtype>FREE</valtype>
    </user_id>

    <gender>
      <descryption>性别</descryption>
      <type>INT</type>
      <length>1</length>
      <valtype>SET</valtype>
      <valmap>gendermap.json</valmap>
    </gender>

    <birthday>
      <descryption>出生日期</descryption>
      <type>INT</type>
      <length>8</length>
      <valtype>DATE</valtype>
    </birthday>

    <degree>
      <descryption>最高学历,值为学历代码</descryption>
      <type>INT</type>
      <length>2</length>
      <valtype>SET</valtype>
      <valmap>degree.json</valmap>
    </degree>

    <major>
      <descryption>当前专业，值为专业代码</descryption>
      <type>INT</type>
      <length>2</length>
      <valtype>SET</valtype>
      <valmap>major.json</valmap>
    </major>

    <location>
      <descryption>所在地区，值为行政区划代码</descryption>
      <type>INT</type>
      <length>4</length>
      <valtype>SET</valtype>
      <valmap>location.json</valmap>
    </location>

    <last_modify>
      <descryption>最后修改时间</descryption>
      <type>INT</type>
      <length>8</length>
      <valtype>TIMESTAMP</valtype>
    </last_modify>
  </user>

</demographics>
